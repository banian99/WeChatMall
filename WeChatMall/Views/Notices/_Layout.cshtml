<!DOCTYPE html>

<html>
<head>
    @*<meta name="viewport" content="width=device-width" />*@
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta name="description" content="Free Web tutorials">
    <meta name="keywords" content="HTML,CSS,JavaScript">
    <meta content="telephone=no" name="format-detection">
   
    <title>@ViewBag.Title</title>
    <link href="~/Common/css/Cbase.css" type="text/css" rel="stylesheet">
    <link href="~/Common/css/index.css" type="text/css" rel="stylesheet">
    <link href="~/Common/css/swiper.min.css" type="text/css" rel="stylesheet">
    <script src="~/Common/js/jquery-1.7.2.min.js" type="text/javascript"></script>
    <script src="~/Common/js/base.js" type="text/javascript"></script>
    <script src="~/Common/js/swiper.min.js" type="text/javascript"></script>
    <script src="~/Common/js/index.js" type="text/javascript"></script>
    <link href="~/Common/css/center.css" type="text/css" rel="stylesheet">

    <script src="~/Common/js/jquery-1.8.0.js"></script>
    @RenderSection("styles",false)
</head>
<body>
    <div>
        @RenderBody()
    </div>
    <!--fixft开始-->
    <div class="fixft">
        <a href="~/Home/Index" class="current" id="current_one" >
            <span><img src="~/Common/images/navicon_45.png" class="img1"><img src="~/Common/images/navicon_28.png" class="img2" ></span>
            <i>首页</i>
        </a>
        <a href="~/ProductSort/Index"  id="current_two">
            <span><img src="~/Common/images/navicon_48.png" class="img1"><img src="~/Common/images/navicon_30.png" class="img2"></span>
            <i>分类</i>
        </a>
        <a href="~/ShopCard/Index"  id="current_one">
            <span><img src="~/Common/images/navicon_50.png" class="img1"><img src="~/Common/images/navicon_32.png" class="img2"><em>12</em></span>
            <i>购物车</i>
        </a>
        <a href="~/UserInfo/Index" id="current_one" >
            <span><img src="~/Common/images/navicon_42.png" class="img1"><img src="~/Common/images/navicon_35.png" class="img2" ></span>
            <i>我的</i>
        </a>
    </div>
    <!--fixft结束-->
   
</body>
</html>
