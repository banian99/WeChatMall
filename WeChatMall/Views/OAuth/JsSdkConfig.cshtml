<!--分布视图中，写js-sdkde配置信息-->
@model Senparc.Weixin.MP.Helpers.JsSdkUiPackage
<script>
    wx.config({
        debug: true, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印。
        appId: '@Model.AppId', // 必填，公众号的唯一标识
        timestamp: @Model.Timestamp, // 必填，生成签名的时间戳
        nonceStr: '@Model.NonceStr', // 必填，生成签名的随机串
        signature: '@Model.Signature',// 必填，签名，见附录1
        jsApiList: ['onMenuShareTimeline','onMenuShareAppMessage','onMenuShareQZone','onMenuShareQQ','chooseImage','openLocation','getLocation','scanQRCode'] // 必填，需要使用的JS接口列表，所有JS接口列表见附录2
    });
    //onMenuShareTimeline :分享朋友圈
    //onMenuShareAppMessage ：分享给朋友
    //onMenuShareQQ ：分享到QQ
    //onMenuShareQZone ：分享到QQ空间
    //chooseImage ：相册接口   previewImage ：预览图片接口
    //openLocation ：获取地理位置查看位置接口
    //getLocation ：获取地理位置
    //scanQRCode ：微信扫一扫

</script>
